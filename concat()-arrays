// First example
function nonMutatingConcat(original, attach) {
  let combined = original.concat(attach);
  return combined;
  
}
var first = [1, 2, 3];
var second = [4, 5];
console.log(nonMutatingConcat(first, second)); // Returns [1, 2, 3, 4, 5]

// Second example
function nonMutatingPush(original, newItem) {
  return original.concat(newItem);
}

var first = [1, 2, 3];
var second = [4, 5];
console.log(nonMutatingPush(first, second)); // Returns [1, 2, 3, 4, 5]
